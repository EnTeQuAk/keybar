; http://editorconfig.org

root = true

[*]
charset = utf-8
indent_style = tabs
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[*.js]
indent_style = space
inden_size = 2

[*.scss]
indent_style = space
inden_size = 2

[*.html]
indent_style = space
inden_size = 2

[*.py]
indent_style = space
indent_size = 4

[*.rst]
indent_style = space
indent_size = 4
